{
  "name": "@jayce789/numjs",
  "version": "0.1.4",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:backends && npm run build:js",
    "build:backends": "npm run build:napi && npm run build:wasm",
    "build:js": "npm --prefix packages/js run build",
    "build:napi": "npm --prefix packages/napi run build",
    "build:napi:cuda": "npm --prefix packages/napi run build:cuda",
    "build:backends:cuda": "npm run build:napi:cuda && npm run build:wasm",
    "build:gpu": "npm run build:backends:cuda && npm run build:js",
    "build:wasm": "npm --prefix packages/wasm run build"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@napi-rs/cli": "^2.16.4",
    "tsup": "^8",
    "typescript": "^5",
    "wasm-pack": "^0.13.1"
  },
  "keywords": [
    "numerical",
    "matrix",
    "linear-algebra",
    "wasm",
    "napi",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "description": ""
}
